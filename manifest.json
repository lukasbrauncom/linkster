{
    "manifest_version": 2,
    "name": "Linkster",
    "version": "0.1",
    
    "description": "Visualizes your local bookmarks",
    "icons": {
    },
    
    "permissions": [
        "http://*/",
        "https://*/",
        "bookmarks",
        "tabs",
        "browserAction"
    ],
    
    "background": {
        "scripts": ["background.js"]
    },
    
    "browser_action": {
        "default_icon": {
            "16": "icons/linkster-16.png",
            "32": "icons/linkster-32.png"
        },
        "default_title": "Linkster"
    }
}
